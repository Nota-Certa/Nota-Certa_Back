{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "projects": {
    "autenticacao": {
      "type": "application",
      "root": "apps/autenticacao",
      "entryFile": "main",
      "sourceRoot": "apps/autenticacao/src",
      "compilerOptions": {
        "tsConfigPath": "apps/autenticacao/tsconfig.app.json"
      }
    },
    "dashboard": {
      "type": "application",
      "root": "apps/dashboard",
      "entryFile": "main",
      "sourceRoot": "apps/dashboard/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dashboard/tsconfig.app.json"
      }
    },
    "notas-fiscais": {
      "type": "application",
      "root": "apps/notas-fiscais",
      "entryFile": "main",
      "sourceRoot": "apps/notas-fiscais/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notas-fiscais/tsconfig.app.json"
      }
    },
    "notificacoes": {
      "type": "application",
      "root": "apps/notificacoes",
      "entryFile": "main",
      "sourceRoot": "apps/notificacoes/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notificacoes/tsconfig.app.json"
      }
    },
    "usuario": {
      "type": "application",
      "root": "apps/usuario",
      "entryFile": "main",
      "sourceRoot": "apps/usuario/src",
      "compilerOptions": {
        "tsConfigPath": "apps/usuario/tsconfig.app.json"
      }
    }
  }
}
